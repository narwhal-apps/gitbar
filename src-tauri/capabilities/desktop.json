{
  "identifier": "desktop-capability",
  "local": true,
  "windows": [
    "main"
  ],
  "platforms": [
    "macOS",
    "windows",
    "linux"
  ],
  "permissions": [
    "updater:default",
    "shell:default",
    "autostart:default",
    "core:default",
    "core:window:allow-set-size",
    "shell:allow-open",
    {
      "identifier": "http:default",
      "allow": [
        {
          "url": "https://**/"
        },
        { "url": "https://*.tauri.app" },
        {
          "url": "https://github.com/*"
        },
        {
          "url": "https://api.github.com/*"
        },
        {
          "url": "http://localhost:44212/*"
        },
        {
          "url": "http://localhost:23212/*"
        },
        {
          "url": "http://localhost:44331/*"
        }
      ]
    },
    "notification:default",
    "notification:default",
    "http:default",
    "shell:default"
  ]
}